<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.malf.upp.mapper.TaskInfoMapper">


    <select id="getTaskInfoById" resultType="com.malf.upp.bean.TaskInfo">
        select * from task_info where id = #{id}
    </select>

    <select id="getAllTaskInfos" resultType="com.malf.upp.bean.TaskInfo">

        select * from task_info

    </select>

</mapper>